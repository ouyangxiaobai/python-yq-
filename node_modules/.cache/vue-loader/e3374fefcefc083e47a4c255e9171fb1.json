\n  border-bottom: 1px solid #dcdfe6;\n}\n\n.index-sales {\n  width: 100%;\n  height: auto;\n  padding: 20px 15px;\n  box-sizing: border-box;\n  /* background: brown; */\n  /* border:  1px solid #E6E6E6; */\n  border-radius: 5px;\n  background: #ffffff;\n  margin-bottom: 20px;\n}\n.index-sales-box {\n  border: 1px solid #e6e6e6;\n  width: 100%;\n  height: auto;\n  position: relative;\n  padding: 25px 15px 5px;\n  box-sizing: border-box;\n}\n.index-sales-box-name {\n  position: absolute;\n  width: 130px;\n  height: 30px;\n  background: #ffffff;\n  display: inline-block;\n  text-align: center;\n  line-height: 30px;\n  font-size: 13px;\n  color: #000000;\n  left: 25px;\n  top: -15px;\n}\n\n.index-head-title {\n  width: 100%;\n  height: 30px;\n  line-height: 30px;\n  font-size: 20px;\n  color: #292929;\n  margin-bottom: 16px;\n}\n.index-head-centent-left {\n  width: 100%;\n  height: 70px;\n  display: flex;\n  justify-content: flex-start;\n  align-items: center;\n}\n.index-head-centent-left-text {\n  height: 56px;\n}\n.index-head-centent-left-text p:nth-child(1) {\n  margin-bottom: 7px;\n  font-size: 20px;\n  color: #292929;\n  letter-spacing: 0.1em;\n}\n.index-head-centent-left-text p:nth-child(2) {\n  font-size: 12px;\n  color: #999999;\n  letter-spacing: 0.1em;\n}\n.index-head-centent-right {\n  width: 100%;\n  height: 70px;\n  display: flex;\n  justify-content: flex-end;\n  align-items: center;\n}\n.index-head-centent-right-list {\n  height: 56px;\n  cursor: pointer;\n  margin-left: 20px;\n}\n.index-head-centent-right-list-icon {\n  font-size: 14px;\n  display: flex;\n  margin-bottom: 5px;\n  color: #808695;\n}\n.index-head-centent-right-list-icon-is {\n  width: 14px;\n  height: 14px;\n  fill: currentColor;\n  margin-right: 5px;\n  padding: 5px;\n  border-radius: 12px;\n}\n\n.index-head-centent-right-list-text {\n  height: 28px;\n  line-height: 28px;\n  font-weight: bold;\n  text-align: right;\n  font-size: 20px;\n}\n.index-centent {\n  width: 100%;\n  min-height: 10